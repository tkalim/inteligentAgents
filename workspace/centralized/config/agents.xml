<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->

	<!-- #### CENTRALIZED AGENTS ### -->
	<!-- java -jar ../logist/logist.jar config/centralized.xml centralized-random -->
	<agent name="centralized-random">
		<set class-path="bin/"/>
		<set class-name="centralized.Centralized"/>
		<set algorithm="NAIVE"/>
	</agent>

	<agent name="centralized-sls">
		<set class-path="bin/"/>
		<set class-name="centralized.Centralized"/>
		<set algorithm="SLS"/>
	</agent>

</agents>
